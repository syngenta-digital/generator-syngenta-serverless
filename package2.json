    "engines": {
        "node": ">=12.14.1"
    },
    "eslintConfig": {
        "env": {
            "es6": true
        },
        "parserOptions": {
            "ecmaVersion": 2018
        },
        "plugins": [
            "prettier"
        ],
        "extends": [
            "airbnb",
            "plugin:prettier/recommended"
        ],
        "rules": {
            "prettier/prettier": [
                "warn",
                {
                    "trailingComma": "none",
                    "printWidth": 120,
                    "tabWidth": 4,
                    "singleQuote": true,
                    "bracketSpacing": false,
                    "arrowParens": "always"
                }
            ],
            "eqeqeq": "error",
            "no-console": 0,
            "guard-for-in": 0,
            "no-await-in-loop": 0,
            "no-restricted-syntax": 0,
            "no-underscore-dangle": 0,
            "camelcase": 0,
            "global-require": 0,
            "no-throw-literal": 0,
            "no-param-reassign": 0,
            "class-methods-use-this": 0,
            "no-use-before-define": 0,
            "no-plusplus": 0,
            "no-undef": 0
        }
    }
