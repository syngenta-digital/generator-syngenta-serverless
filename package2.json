    "scripts": {
        "lint": "eslint application/**",
        "sct": "sct",
        "report": "nyc report",
        "test": "unittest=true nyc mocha --recursive",
        "start": "concurrently \"docker-compose -f aws/local/neo4j.yml up -d\" \"serverless offline start --stage local --aws_envs local --profile local --region us-east-2\"",
        "version": "serverless invoke local --function v1-console-database-versioner --stage local --aws_envs local --region us-east-2",
        "test2": "concurrently \"docker-compose -f aws/local/neo4j.yml up -d\" \"serverless offline start --stage local --aws_envs local --profile local --region us-east-2\""
    },
    "author": "",
    "license": "ISC",
